MODULE "myData.module";// file name
Employees = GenData.genEmployee
    (
    );
Departments =
    GenData.genDepartment
    (
    );

OUTPUT Employees
TO "my/myData.txt";

All =
    SELECT e.EmpName AS EmpName,
           e.DeptId AS DeptId,
           d.DepName
    FROM Employees AS e,
         Departments AS d
    WHERE e.DeptId == d.DeptId;

OUTPUT All
TO "my/All.txt";