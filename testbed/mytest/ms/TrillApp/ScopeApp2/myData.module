DEFINE MODULE GenData
BEGIN

VIEW gen
RETURN ROWSET(EmpName:string, DeptId:int)
PARAMS();
END MODULE

VIEW gen
RETURN ROWSET(EmpName:string, DeptId:int)
PARAMS()
BEGIN
