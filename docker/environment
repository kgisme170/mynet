PATH=$GOPATH/bin:$GOROOT/bin:$PATH:/usr/local/bin:.
CLASSPATH=$JAVA_HOME/lib:.

C_INCLUDE_PATH=$JAVA_HOME/include:$JAVA_HOME/include/linux:/usr/include/llvm-c-8/llvm-c/Transforms:/usr/include/python2.7:/usr/include/tcl8.6:/thrift-0.10.0/lib/cpp/src:/grpc/include:.
CPLUS_INCLUDE_PATH=$JAVA_HOME/include:$JAVA_HOME/include/linux:/usr/include/llvm-c-8/llvm-c/Transforms:/usr/include/python2.7:/usr/include/tcl8.6:/thrift-0.10.0/lib/cpp/src:/grpc/include:.
